<% statusHelper = (status, link) => {%>
    <a href="<%=link%>" class="my-btn-state rounded-circle btn btn-sm btn-success"><i class="fas fa-check"></i>
        <%if(status==1) {%>
            <span>active</span>
        <% } else if (status==0) { %>
             <span>inactive</span>
        <%}%>
    </a>
<%}%>
